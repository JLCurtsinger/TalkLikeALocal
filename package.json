{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node scripts/expose-css.mjs && npm run build:blog && npm run postbuild:merge-blog && node scripts/verify-blog-css.mjs",
    "build:blog": "npm --prefix ./blog ci --no-audit --no-fund --prefer-offline && npm --prefix ./blog run build",
    "postbuild:merge-blog": "mkdir -p dist/blog && cp -R blog/dist/* dist/blog && cp blog/public/blog.css dist/blog/ || true && echo '[postbuild:merge-blog] completed successfully'",
    "lint": "eslint .",
    "preview": "vite preview",
    "seo:build": "node scripts/discover-routes.mjs && node scripts/prerender.mjs && node scripts/generate-sitemap.mjs",
    "seo:preview": "vite preview --port 4173 --strictPort",
    "seo:routes": "node -e \"import('fs').then(fs => { console.log(fs.readFileSync('scripts/prerender.routes.json','utf8')); process.exit(0); })\""
  },
  "dependencies": {
    "@tsparticles/engine": "^3.3.0",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.3.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.8",
    "lodash": "^4.17.21",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.22.3",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "fs-extra": "^11.3.1",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "puppeteer": "^24.18.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
